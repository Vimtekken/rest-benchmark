{
  "author": "Vimtekken",
  "dependencies": {
    "debug": "4.1.1",
    "influx": "5.5.1"
  },
  "description": "Benchmarking the HTTP handling of multiple REST frameworks",
  "devDependencies": {
    "esdoc": "1.1.0",
    "esdoc-node": "1.0.5",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "npm-check-updates": "4.0.1",
    "typescript": "3.8.2"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "main": "build/src/app.js",
  "name": "rest-benchmark",
  "scripts": {
    "build": "tsc && npm run copy_config",
    "copy_config": "cp src/applications_config.json build/src/applications_config.json",
    "docker_build": "docker build . -t rest-benchmark:latest",
    "docker_run": "docker run --env-file local.env -v /var/run/docker.sock:/var/run/docker.sock rest-benchmark:latest",
    "start": "node build/src/app.js"
  },
  "version": "0.0.1"
}
